<?xml version="1.0" encoding="UTF-8"?>
<RScript xmlns="http://www.fao.org/rScript" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	       xsi:schemaLocation="http://www.fao.org/rScript rScript.xsd ">
	<Name>Impute Livestock</Name>
	<Description>
    The module impute and synchronise the livestock tree. The imputation is
    performed at the meat commodity, and then the slaughtered animal is
    synchronised with other related livestock commodities.
    
	</Description>
	<Category>Imputation</Category>
	<Authors>
		<Author>
			<Name>Michael C. J. Kao</Name>
			<Mail>michael.kao@fao.org</Mail>
		</Author>
	</Authors>
  <Params>
		<Param xsi:type="List">
	    <Code>imputation_selection</Code>
	    <Label>Perform Imputation on </Label>
		  <Description>
			  Please specify the selection to perform imputation.
		  </Description>
		  <Compulsory>true</Compulsory>
      <Options>
				<Option>
					<Label> Selected session </Label>
					<Value>session</Value>
				</Option>
				<Option>
					<Label> All Production Data </Label>
					<Value>all</Value>
				</Option>
			</Options>
    </Param>
  </Params>
	<Datasets>
		<MainDataset>
			<Code>aproduction</Code>
			<SelectableDimensions>
				<SelectableDimension compulsory="false">geographicAreaM49</SelectableDimension>
				<SelectableDimension compulsory="true">measuredItemCPC</SelectableDimension>
				<SelectableDimension compulsory="false">measuredElement</SelectableDimension>
				<SelectableDimension compulsory="false">timePointYears</SelectableDimension>
			</SelectableDimensions>
		</MainDataset>
	</Datasets>
	<InteractiveValidation>false</InteractiveValidation>
	<Background>false</Background>
</RScript>
