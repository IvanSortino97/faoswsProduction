<?xml version="1.0" encoding="UTF-8"?>
<RScript xmlns="http://www.fao.org/rScript" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.fao.org/rScript rScript.xsd ">
	<Name>Build Production Imputation Model</Name>
	<Description>Build the model for production imputation</Description>
	<Category>Computation</Category>
	<Authors>
		<Author>
			<Name>Josh Browning</Name>
			<Mail>joshua.browning@fao.org</Mail>
		</Author>
	</Authors>
  <Params>
		<Param xsi:type="Number">
	        <Code>firstYear</Code>
	            <Label>First Year for model building</Label>
		        <Description>
			    Please specify the first year for which the data should be pulled to construct the imputation model (1991 recommended).
		        </Description>
		        <Compulsory>true</Compulsory>
                <Min>1991</Min>
                <Max>2050</Max>
   </Param>
		<Param xsi:type="Number">
	        <Code>lastYear</Code>
	            <Label>Last Year for model building</Label>
		        <Description>
			    Please specify the last year for which the data should be pulled to construct the imputation model.  Note that data cannot be imputed outside this range.
		        </Description>
		        <Compulsory>true</Compulsory>
                <Min>1991</Min>
                <Max>2050</Max>
   </Param>
  </Params>
	<Datasets>
		<MainDataset>
			<Code>aproduction</Code>
			<SelectableDimensions>
				<SelectableDimension compulsory="true">geographicAreaM49</SelectableDimension>
				<SelectableDimension compulsory="true">measuredItemCPC</SelectableDimension>
				<SelectableDimension compulsory="true">measuredElement</SelectableDimension>
				<SelectableDimension compulsory="true">timePointYears</SelectableDimension>
			</SelectableDimensions>
		</MainDataset>
	</Datasets>
	<InteractiveValidation>false</InteractiveValidation>
	<Background>false</Background>
</RScript>